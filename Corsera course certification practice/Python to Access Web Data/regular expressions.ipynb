    "fname = 'mbox-short.txt'\n",
    "fh = open(fname)\n",
    "s=0\n",
    "count=0\n",
    "for line in fh:\n",
    "    if line.startswith(\"X-DSPAM-Confidence:\") :\n",
    "        stnum = line[line.find(\"0\") : len(line)]\n",
    "        fum = float(stnum)\n",
    "        s = s+fum\n",
    "        count +=1\n",
    "print(\"Average spam confidence:\", s/count)"
   ]
